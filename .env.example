# Rename this file to .env and fill in your credentials
# ProtonMail SMTP settings
SMTP_HOST=smtp.protonmail.com
SMTP_PORT=465
SMTP_USER=your-protonmail-username@proton.me
SMTP_PASS=your-protonmail-app-password
FROM_EMAIL=your-protonmail-username@proton.me

# Twilio credentials (optional for SMS notifications)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890
